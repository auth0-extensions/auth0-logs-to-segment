{
  "name": "auth0-logs-to-segment",
  "version": "1.1.0",
  "description": "This extension will take all of your Auth0 logs and export them to Segment",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run client:build && npm run extension:build",
    "clean": "rimraf dist",
    "client:build": "npm run clean && cross-env NODE_ENV=production webpack --config ./build/webpack/config.prod.js --colors -p",
    "extension:build": "a0-ext build:server ./webtask.js ./dist && cp ./dist/auth0-sso-dashboard.extension.$npm_package_version.js ./build/bundle.js",
    "lint:js": "eslint --ignore-path .gitignore --ignore-pattern build --ignore-pattern webpack .",
    "serve:dev": "cross-env NODE_ENV=development gulp run --gulpfile ./build/gulpfile.js",
    "serve:prod": "cross-env NODE_ENV=production node index.js",
    "tag": "git tag $npm_package_version && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auth0/auth0-logs-to-segment.git"
  },
  "author": "yamil@auth0.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auth0/auth0-logs-to-segment/issues"
  },
  "homepage": "https://github.com/auth0/auth0-logs-to-segment#readme",
  "auth0-extension": {
    "externals": [
      "analytics-node",
      "async",
      "auth0",
      "auth0-extension-express-tools",
      "auth0-extension-tools",
      "auth0-log-extension-tools",
      "auth0-oauth2-express",
      "bluebird",
      "body-parser",
      "express",
      "express-jwt",
      "jsonwebtoken",
      "jwt-decode",
      "lodash",
      "lru-memoizer",
      "moment",
      "morgan",
      "nconf",
      "qs",
      "request",
      "request-promise",
      "useragent",
      "webtask-tools",
      "winston"
    ]
  },
  "dependencies": {
    "analytics-node": "3.2.0",
    "async": "^1.5.2",
    "auth0": "^2.4.0",
    "auth0-extension-express-tools": "1.1.0",
    "auth0-extension-tools": "1.2.1",
    "auth0-log-extension-tools": "1.2.2",
    "auth0-oauth2-express": "^1.1.8",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
